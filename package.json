{
  "name": "expensesdashboard-server",
  "version": "1.0.0",
  "description": "Expenses Dashboard Server\" is a backend service providing APIs for tracking and managing personal and group expenses with a user-friendly interface.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "build": "tsc",
    "start-pm2": "pm2 start ecosystem.config.cjs --env production",
    "stop-pm2": "pm2 stop ecosystem.config.cjs",
    "restart-pm2": "pm2 restart ecosystem.config.cjs",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@vitest/ui": "^1.6.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@google-cloud/dialogflow": "^6.7.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
