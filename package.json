    "express-yields": "^1.1.2",
    "http-status": "^1.7.4",
    "ioredis": "^5.4.1",
    "mongoose": "^8.4.1",
    "nodemon": "^3.1.3",
    "pm2": "^5.4.1",
    "redis": "^4.6.14",
    "socket.io": "^4.7.5",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  }
